<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hosteleriapp" >

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_hosteleriapp"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_hosteleriapp_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HosteleriApp" >
        <activity
            android:name=".EstablecimientoActivity"
            android:exported="false"
            android:label="@string/title_activity_establecimiento"
            android:theme="@style/Theme.HosteleriApp.NoActionBar" />
        <activity
            android:name=".Bar.BarActivity"
            android:exported="false" />
        <activity
            android:name=".Usuario.UsuarioActivity"
            android:exported="false" />
        <activity
            android:name=".Administrador.AdminActivity"
            android:exported="false" />
        <activity
            android:name=".RegistrarActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>